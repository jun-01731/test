drop database if exists sample1db;
create database sample1db;
use sample1db;

create table sample1_table(
No. int,
name varchar(50),
age int,
Mail varchar(50)
);

insert into sample1_table values(1,"Yamada",21,"xxx@yahoo.co.jp");
